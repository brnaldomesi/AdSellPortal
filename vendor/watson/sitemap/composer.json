{
    "name": "watson/sitemap",
    "description": "Generate Google Sitemaps in Laravel 4/5",
    "keywords": ["laravel", "sitemaps"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@studious.com.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src/Watson"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Sitemap\\SitemapServiceProvider"
            ],
            "aliases": {
                "Sitemap": "Watson\\Sitemap\\Facades\\Sitemap"
            }
        }
    }
}