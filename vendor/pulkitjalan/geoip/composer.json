{
    "name": "pulkitjalan/geoip",
    "description": "Geoip Wrapper with Laravel 4 & 5 Support",
    "homepage": "https://github.com/pulkitjalan/geoip",
    "keywords": ["geoip", "freegeoip", "ip-api", "maxmind", "telize"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pulkit Jalan"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/support": "~5",
        "illuminate/console": "~5",
        "guzzlehttp/guzzle": "~6",
        "geoip2/geoip2": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "PulkitJalan\\GeoIP\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7.x-dev"
        },
        "laravel": {
            "providers": [
                "PulkitJalan\\GeoIP\\GeoIPServiceProvider"
            ],
            "aliases": {
                "GeoIP": "PulkitJalan\\GeoIP\\Facades\\GeoIP"
            }
        }
    }
}
