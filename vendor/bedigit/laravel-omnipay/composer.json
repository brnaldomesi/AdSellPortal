{
    "name": "bedigit/laravel-omnipay",
    "description": "Integerates Omnipay with Laravel and provides an easy configuration.",
    "keywords": ["omnipay", "payments", "laravel", "laravel5", "larapen"],
    "authors": [
        {
            "name": "Alex Whiteside",
            "email": "alexwhiteside@ignitedlabs.com.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5",
        "omnipay/common": "^3@beta"
    },
    "autoload": {
        "psr-0": {
            "Ignited\\LaravelOmnipay": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ignited\\LaravelOmnipay\\LaravelOmnipayServiceProvider"
            ],
            "aliases": {
                "Omnipay": "Ignited\\LaravelOmnipay\\Facades\\OmnipayFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
